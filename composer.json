{
    "name": "kafkiansky/teleport",
    "description": "Another port to telegram API.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "amphp/http-client": "^5.0",
        "cuyz/valinor": "^1.4",
        "amphp/sync": "2.x-dev",
        "league/uri": "^6.0@dev",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "1.11.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teleport\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Teleport\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "v.zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "lint": "vendor/bin/phpstan analyse src tests"
    }
}
